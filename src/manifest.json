{
    "name": "webinfo",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16": "assets/icon16.png",
            "48": "assets/icon48.png",
            "128": "assets/icon128.png"
        },
        "default_title": "Vue Popup"
    },
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "background": {
        "service_worker": "background.ts"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content-scripts/main.ts"],
            "css": ["content-scripts/main.css"]
        }
    ],
    "options_page": "devtools/index.html",
    "devtools_page": "devtools/index.html",
    "permissions": [
        "contextMenus",
        "declarativeContent", "activeTab", "storage", "tabs",
        "webRequest"
    ],
    "web_accessible_resources": [
        {
            "resources": [ "inject.js" ],
            "matches": [ "<all_urls>" ]
        }
    ]
}
